. 
. * Open Stata dataset
. use $tempdir\analysis_dataset_STSET_$outcome, clear

. 
. /* Print table================================================================*/ 
. *  Print the results for the main model 
. 
. cap file close tablecontent

. file open tablecontent using ./$outdir/table5.txt, write text replace

. 
. * Column headings 
. file write tablecontent ("Table 5: 1 by 1 comorbidity adjustments (after age/sex and strata adjustments) - 
> $population population") _n

. file write tablecontent _tab ("HR") _tab ("95% CI") _n

. 
. /* Adjust one covariate at a time=============================================*/
. 
. foreach var of varlist  obese4cat                               ///
>                                                 asthma_ever                             ///
>                                                 smoke_nomiss                    ///
>                                                 imd                                     ///
>                                                 ckd                                             ///        
>      
>                                                 hypertension                    ///             
>                                                 heart_failure                   ///             
>                                                 other_heart_disease             ///             
>                                                 diabcat                                 ///             
>                                                 cancer_ever                     ///                        
>                              
>                                                 statin                                  ///                
>      
>                                                 oral_steroids                   ///             
>                                                 flu_vaccine                     ///     
>                                                 pneumococcal_vaccine    ///     
>                                                 exacerbations                   ///
>                                                 gp_consult { 
  2.         
.         local lab: variable label `var'
  3.         file write tablecontent ("`lab'") _n 
  4.         
.         qui stcox i.exposure i.male age1 age2 age3 i.`var', strata(stp) 
  5.                 
.                 local lab0: label exposure 0
  6.                 local lab1: label exposure 1
  7. 
.                 file write tablecontent ("`lab0'") _tab
  8.                 file write tablecontent ("1.00 (ref)") _tab _n
  9.                 file write tablecontent ("`lab1'") _tab  
 10.                 
.                 qui lincom 1.exposure, eform
 11.                 file write tablecontent %4.2f (r(estimate)) _tab %4.2f (r(lb)) (" - ") %4.2f (r(ub)) _n 
> _n
 12.                                                                         
. }       

. 
. file write tablecontent _n

. file close tablecontent

